#!/usr/bin/env ruby

require 'bundler/setup'
require 'evolvable_sound'


$VERBOSE = nil

EvolvableSound.client = Client::CommandLine
EvolvableSound.client.display_program_name

population_attrs = {
  name: Time.now.utc.to_i.to_s,
  size: 5,
}
population = EvolvableSound.new_population(population_attrs)
population.mutation.rate = 0.2
population.evolve!
